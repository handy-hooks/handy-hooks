    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@testing-library/react-hooks": "^5.0.3",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
